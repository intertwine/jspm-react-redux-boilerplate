{
  "name": "jspm-react-redux-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for a React application with Redux. Uses JSPM for package management and SystemJS hot reloader for hot reloading.",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "postinstall": "jspm install",
    "build": "jspm bundle index --inject --minify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonahlau/jspm-react-redux-boilerplate.git"
  },
  "keywords": [
    "jspm",
    "react",
    "redux",
    "boilerplate"
  ],
  "author": "Jonathan Lau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonahlau/jspm-react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/jonahlau/jspm-react-redux-boilerplate#readme",
  "dependencies": {
    "cors": "^2.7.1",
    "express": "^4.13.4"
  },
  "devDependencies": {
    "body-parser": "^1.14.2",
    "chokidar-socket-emitter": "^0.4.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "jspm": "^0.16.19",
    "jwt-simple": "^0.4.1",
    "nodemon": "^1.8.1"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "dependencies": {
      "axios": "npm:axios@^0.9.0",
      "history": "npm:history@^2.0.0-rc2",
      "immutable": "npm:immutable@^3.7.6",
      "react": "npm:react@^0.14.6",
      "react-dom": "npm:react-dom@^0.14.6",
      "react-redux": "npm:react-redux@^4.0.6",
      "react-router": "npm:react-router@^2.0.0-rc5",
      "redux": "npm:redux@^3.0.5",
      "redux-simple-router": "npm:redux-simple-router@^2.0.4",
      "redux-thunk": "npm:redux-thunk@^1.0.3"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "capaj/systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@0.5.1",
      "core-js": "npm:core-js@^1.1.4",
      "redux-logger": "npm:redux-logger@^2.3.2"
    },
    "buildConfig": {
      "transpileES6": true,
      "minify": true
    }
  }
}
